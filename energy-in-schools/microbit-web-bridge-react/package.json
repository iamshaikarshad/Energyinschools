    "jspath": "^0.4.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "loglevel-plugin-prefix": "^0.8.4",
    "modernizr-webpack-plugin": "^1.0.7",
    "moment": "^2.22.2",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "react": "16.8.0",
    "react-dock": "^0.2.4",
    "react-dom": "16.8.0",
    "react-hot-loader": "^4.11.1",
    "react-material-ui-form-validator": "^2.0.9",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
